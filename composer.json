{
    "name": "mostafaznv/nova-morph-filter",
    "description": "Morph Filter for Laravel Nova",
    "keywords": ["laravel", "nova", "morph", "filter"],
    "license": "MIT",
    "require": {
        "php": "^8.0",
        "laravel/framework": "^8.0|^9.0"
    },
    "autoload": {
        "psr-4": {
            "Mostafaznv\\NovaMorphFilter\\": "src/"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "Mostafaznv\\NovaMorphFilter\\NovaMorphFilterServiceProvider"
            ]
        }
    },
    "config": {
        "sort-packages": true
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
